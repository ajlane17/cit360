<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd"
           version="4.0">

    <servlet>
        <description></description>
        <display-name>HelloWorldConsole</display-name>
        <servlet-name>HelloWorldConsole</servlet-name>
        <servlet-class>com.adrianjlane.servlet.HelloWorldConsole</servlet-class>
    </servlet>
    <servlet>
        <description></description>
        <display-name>HelloWorldHtml</display-name>
        <servlet-name>HelloWorldHtml</servlet-name>
        <servlet-class>com.adrianjlane.servlet.HelloWorldHtml</servlet-class>
    </servlet>
    <servlet>
        <description></description>
        <display-name>ParamsExample</display-name>
        <servlet-name>ParamsExample</servlet-name>
        <servlet-class>com.adrianjlane.servlet.ParamsExample</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloWorldConsole</servlet-name>
        <url-pattern>/HelloWorldConsole</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HelloWorldHtml</servlet-name>
        <url-pattern>/HelloWorldHtml</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ParamsExample</servlet-name>
        <url-pattern>/ParamsExample</url-pattern>
    </servlet-mapping>

</web-app>
